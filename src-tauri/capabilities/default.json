{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions for Claude Setting Dashboard",
  "windows": [
    "main",
    "preview"
  ],
  "permissions": [
    "core:default",
    "core:event:allow-emit",
    "core:event:allow-emit-to",
    "core:event:allow-listen",
    "core:event:allow-unlisten",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-copy-file",
    "fs:allow-read-dir",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
